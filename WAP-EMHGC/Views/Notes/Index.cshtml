@model IEnumerable<WAP_EMHGC.Models.Note>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoteAbout)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoteNews)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoteObj)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Note1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AccId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoteAbout)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoteNews)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoteObj)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Note1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NoteId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NoteId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NoteId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
